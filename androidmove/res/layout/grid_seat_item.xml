<?xml version="1.0" encoding="utf-8"?>
<!-- 表头S -->
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
	 android:layout_width="wrap_content"
	android:layout_height="wrap_content">
	<TextView android:layout_width="20dip" android:textSize="16sp"
		android:textColor="#6A6862" android:textStyle="bold" android:id="@+id/seatnum"
		android:ellipsize="end" android:singleLine="true" android:visibility="invisible"
		android:layout_centerInParent="true" android:layout_height="20dip"
		android:textAppearance="?android:attr/textAppearanceMedium"
		android:gravity="center" android:text="1" />
	<ImageView android:layout_width="20dip" android:background="@drawable/seatimage"
		android:visibility="invisible" 
		android:layout_height="20dip" android:id="@+id/seatimage"
		android:layout_centerInParent="true" android:layout_gravity="center_vertical" />
	<TextView android:layout_width="20dip" android:textSize="16sp"
		android:textColor="#6A6862" android:textStyle="bold" android:id="@+id/textspace"
		android:ellipsize="end" android:singleLine="true"
		android:layout_centerInParent="true" android:layout_height="20dip"
		android:visibility="invisible" android:textAppearance="?android:attr/textAppearanceMedium"
		android:gravity="center" android:text=" " />
</RelativeLayout>
<!-- 表头E -->