package com.szcmcc.movie.map;


import java.io.Serializable;

import com.mapabc.mapapi.core.GeoPoint;

/**
 * 
 * @author huangyu
 * 2011-5-20
 */
public class MapPoi implements Serializable {

	private String price;//价格
	private String discount;//折扣
	private String desc;//秒素
	private String stopTime;//剩余时间
	private String leave_days;//剩天数
	private String sellCount;//售出数量
	private String url;//图片的url
	private String id;
	private String value;//原价
	private String title;//原价
	
	private GeoPoint mGeoPoint;
	private String lat;//经度
	private String lng;//纬度
	private int type;//商品类型，
	private String tuan_name;//团名称，
	
	private String comment_num;
	private String comment_url;
	private String detail_url;
	private String merchant;//商家名称
	private String merchant_addr;//商家地址
	private String merchant_tel;//商家电话
	private String siteurl;//商家电话
	private String goods_url;//商家电话
	private String time;//实时团购
	private double Distance;//实时团购
	
	public MapPoi(String price, String discount, String desc, String stopTime, String sellCount, String url) {
		super();
		this.price = price;
		this.discount = discount;
		this.desc = desc;
		this.stopTime = stopTime;
		this.sellCount = sellCount;
		this.url = url;
	}
	
	public double getDistance() {
		return Distance;
	}

	public void setDistance(double distance) {
		Distance = distance;
	}

	public String getTitle() {
		return title;
	}

	public String getGoods_url() {
		return goods_url;
	}

	public void setGoods_url(String goodsUrl) {
		goods_url = goodsUrl;
	}

	public String getTime() {
		return time;
	}

	public void setTime(String time) {
		this.time = time;
	}

	public void setTitle(String title) {
		this.title = title;
	}

	public String getLeave_days() {
		return leave_days;
	}

	public void setLeave_days(String leaveDays) {
		leave_days = leaveDays;
	}

	public MapPoi() {
	}
	
	public String getSiteurl() {
		return siteurl;
	}
	public void setSiteurl(String siteurl) {
		this.siteurl = siteurl;
	}
	public String getComment_num() {
		return comment_num;
	}
	public void setComment_num(String commentNum) {
		comment_num = commentNum;
	}
	public String getComment_url() {
		return comment_url;
	}
	public void setComment_url(String commentUrl) {
		comment_url = commentUrl;
	}
	public String getDetail_url() {
		return detail_url;
	}
	public void setDetail_url(String detailUrl) {
		detail_url = detailUrl;
	}
	public String getMerchant() {
		return merchant;
	}
	public void setMerchant(String merchant) {
		this.merchant = merchant;
	}
	public String getMerchant_addr() {
		return merchant_addr;
	}
	public void setMerchant_addr(String merchantAddr) {
		merchant_addr = merchantAddr;
	}
	public String getMerchant_tel() {
		return merchant_tel;
	}
	public void setMerchant_tel(String merchantTel) {
		merchant_tel = merchantTel;
	}
	public String getValue() {
		return value;
	}
	public void setValue(String value) {
		this.value = value;
	}
	public String getId() {
		return id;
	}
	public void setId(String id) {
		this.id = id;
	}
	public String getTuan_name() {
		return tuan_name;
	}
	public void setTuan_name(String tuanName) {
		tuan_name = tuanName;
	}
	public int getType() {
		return type;
	}
	public void setType(int type) {
		this.type = type;
	}
	public GeoPoint getmGeoPoint() {
		return mGeoPoint;
	}
	public void setmGeoPoint(GeoPoint mGeoPoint) {
		this.mGeoPoint = mGeoPoint;
	}
	public String getLat() {
		return lat;
	}
	public void setLat(String lat) {
		this.lat = lat;
	}
	public String getLng() {
		return lng;
	}
	public void setLng(String lng) {
		this.lng = lng;
	}
	public String getPrice() {
		return price;
	}
	public void setPrice(String price) {
		this.price = price;
	}
	public String getDiscount() {
		return discount;
	}
	public void setDiscount(String discount) {
		this.discount = discount;
	}
	public String getDesc() {
		return desc;
	}
	public void setDesc(String desc) {
		this.desc = desc;
	}
	public String getStopTime() {
		return stopTime;
	}
	public void setStopTime(String stopTime) {
		this.stopTime = stopTime;
	}
	public String getSellCount() {
		return sellCount;
	}
	public void setSellCount(String sellCount) {
		this.sellCount = sellCount;
	}
	public String getUrl() {
		return url;
	}
	public void setUrl(String url) {
		this.url = url;
	}
	@Override
	public String toString() {
		return "Goods [desc=" + desc + ", discount=" + discount + ", id=" + id + ", lat=" + lat + ", lng=" + lng
				+ ", mGeoPoint=" + mGeoPoint + ", price=" + price + ", sellCount=" + sellCount + ", stopTime="
				+ stopTime + ", tuan_name=" + tuan_name + ", type=" + type + ", url=" + url + "]";
	}
}
