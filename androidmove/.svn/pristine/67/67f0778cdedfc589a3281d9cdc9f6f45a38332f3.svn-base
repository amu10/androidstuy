package com.szcmcc.movie.bean;

import java.io.Serializable;

/**
 * 单个电影座位的信息
 * @author Haylee
 *
 */
public class SeatInfo implements Serializable{
	
	/**
	 * 
	 */
	private static final long serialVersionUID = 1957884225689822823L;
	public String seatCode;
	public String seatAreaCode;
	public String graphRow; //屏幕行
	public String graphCol;//屏幕咧
	public String seatRow;
	public String seatCol;
	public String damageFlg;//座位维修标记
	public String seatState;//座位状态
	@Override
	public String toString() {
		return "SeatInfo [seatCode=" + seatCode + ", seatAreaCode="
				+ seatAreaCode + ", graphRow=" + graphRow + ", graphCol="
				+ graphCol + ", seatRow=" + seatRow + ", seatCol=" + seatCol
				+ ", damageFlg=" + damageFlg + ", seatState=" + seatState + "]";
	}

}
