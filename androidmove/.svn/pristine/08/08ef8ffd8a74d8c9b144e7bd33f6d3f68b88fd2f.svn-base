<?xml version="1.0" encoding="utf-8"?>
<!-- 表头S -->
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
	android:orientation="horizontal" android:layout_width="fill_parent"
	android:layout_height="wrap_content">
	<TextView android:layout_width="23dip" android:textSize="16sp"
		android:textColor="#6A6862" android:textStyle="bold"
		android:ellipsize="end" android:singleLine="true" android:visibility="invisible"
		android:layout_gravity="center_vertical" android:layout_height="23dip"
		android:textAppearance="?android:attr/textAppearanceMedium"
		android:gravity="center" android:text=" " />
	<TextView android:layout_width="23dip" android:textSize="16sp"
		android:textColor="#6A6862" android:textStyle="bold"
		android:ellipsize="end" android:singleLine="true"
		android:layout_gravity="center_vertical" android:layout_height="23dip"
		android:textAppearance="?android:attr/textAppearanceMedium"
		android:gravity="center" android:text="2" />
	<TextView android:layout_width="23dip" android:textSize="16sp"
		android:textColor="#6A6862" android:textStyle="bold"
		android:ellipsize="end" android:singleLine="true"
		android:layout_gravity="center_vertical" android:layout_height="23dip"
		android:textAppearance="?android:attr/textAppearanceMedium"
		android:gravity="center" android:text="4" />
	<RelativeLayout android:orientation="horizontal"
		android:layout_weight="1.0" android:layout_width="fill_parent"
		android:layout_height="fill_parent">
		<!-- 动态添加座位 -->
		<LinearLayout android:orientation="horizontal"
			android:id="@+id/seat_lable_pl" android:layout_centerHorizontal="true"
			android:layout_width="wrap_content" android:layout_height="fill_parent">
			<TextView android:layout_width="23dip" android:textSize="16sp"
				android:textColor="#6A6862" android:textStyle="bold"
				android:ellipsize="end" android:singleLine="true"
				android:layout_gravity="center_vertical" android:layout_height="23dip"
				android:textAppearance="?android:attr/textAppearanceMedium"
				android:gravity="center" android:text="4" />
		</LinearLayout>
	</RelativeLayout>
	<TextView android:layout_width="23dip" android:textSize="16sp"
		android:textColor="#6A6862" android:textStyle="bold"
		android:ellipsize="end" android:singleLine="true"
		android:layout_gravity="center_vertical" android:layout_height="23dip"
		android:textAppearance="?android:attr/textAppearanceMedium"
		android:gravity="center" android:text="3" />
	<TextView android:layout_width="23dip" android:textSize="16sp"
		android:textColor="#6A6862" android:textStyle="bold"
		android:ellipsize="end" android:singleLine="true"
		android:layout_gravity="center_vertical" android:layout_height="23dip"
		android:textAppearance="?android:attr/textAppearanceMedium"
		android:gravity="center" android:text="1" />
	<TextView android:layout_width="23dip" android:textSize="16sp"
		android:textColor="#6A6862" android:textStyle="bold"
		android:ellipsize="end" android:singleLine="true" android:visibility="invisible"
		android:layout_gravity="center_vertical" android:layout_height="23dip"
		android:textAppearance="?android:attr/textAppearanceMedium"
		android:gravity="center" android:text=" " />
</LinearLayout>
<!-- 表头E -->