    <LinearLayout
        android:id="@+id/homepage_layout_score"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_alignParentRight="true"
        android:layout_gravity="right"
        android:layout_marginBottom="12dip"
        android:orientation="horizontal" >

        <RatingBar
            android:id="@+id/homepage_ratingBar_score"
            style="@style/scoreRatingBar"
            android:layout_width="wrap_content"
            android:layout_height="23dip"
            android:layout_gravity="center_vertical"
            android:layout_marginRight="5dip"
            android:focusable="false"
            android:gravity="center_vertical"
            android:isIndicator="true"
            android:numStars="5"
            android:rating="0" />

        <TextView
            android:id="@+id/homepage_textView_score"
            android:layout_width="wrap_content"
            android:layout_height="21dip"
            android:layout_marginBottom="0dip"
            android:layout_marginRight="5dip"
            android:gravity="center"
            android:text="0.0"
            android:textColor="#DBB502"
            android:textSize="16sp"
            android:textStyle="bold" />
    </LinearLayout>

    <RelativeLayout
        android:id="@+id/homepage_layout_moviename"
        android:layout_width="fill_parent"
        android:layout_height="50dip"
        android:layout_alignParentBottom="true"
        android:layout_gravity="right"
        android:layout_marginBottom="23dip"
        android:orientation="horizontal" >

        <TextView
            android:id="@+id/homepage_textView_moviename"
            android:layout_width="230dip"
            android:layout_height="30dip"
            android:layout_marginTop="10dip"
            android:ellipsize="end"
            android:gravity="center"
            android:singleLine="true"
            android:text=""
            android:textColor="#FDFAF9"
            android:textSize="20sp"
            android:textStyle="bold" />

        <TextView
            android:id="@+id/homepage_textView_buyticket"
            android:layout_width="80dip"
            android:layout_height="40dip"
            android:layout_alignParentRight="true"
            android:layout_marginBottom="12dip"
            android:gravity="center"
            android:text=""
            android:textColor="#010101"
            android:textSize="18sp"
            android:textStyle="bold" />
    </RelativeLayout>
    <!-- 更多影片S -->

    <RelativeLayout
        android:id="@+id/more_movie_relative"
        android:layout_width="wrap_content"
        android:layout_height="fill_parent"
        android:layout_alignParentRight="true"
        android:layout_alignWithParentIfMissing="true" >

        <ListView
            android:id="@+id/more_listview"
            android:layout_width="90dip"
            android:layout_height="fill_parent"
            android:layout_alignParentRight="true"
            android:layout_gravity="center_horizontal"
            android:background="@drawable/black"
            android:cacheColorHint="#00000000"
            android:divider="@null"
            android:fadingEdge="none"
            android:footerDividersEnabled="false"
            android:listSelector="#00000000"
            android:scrollbars="none"
            android:visibility="gone" />

        <LinearLayout
            android:id="@+id/listview_menuLayout"
            android:layout_width="50dip"
            android:layout_height="108dip"
            android:layout_alignWithParentIfMissing="true"
            android:layout_marginTop="50dip"
            android:layout_toLeftOf="@id/more_listview"
            android:gravity="right"
            android:scaleType="center" >

            <ImageView
                android:id="@+id/listview_menu"
                android:layout_width="32dip"
                android:layout_height="108dip"
                android:background="@drawable/more_film" />
        </LinearLayout>
    </RelativeLayout>
    <!-- 更多影片E -->

    <RelativeLayout
        android:id="@+id/noHot_movie_relative"
        android:layout_width="wrap_content"
        android:layout_height="fill_parent"
        android:layout_alignParentRight="true"
        android:layout_alignWithParentIfMissing="true" >

        <ListView
            android:id="@+id/noHot_listview"
            android:layout_width="90dip"
            android:layout_height="fill_parent"
            android:layout_alignParentRight="true"
